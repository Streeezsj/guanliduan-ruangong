n            .addhumanlist(this.item.itemId,this.selects)\n            .then(function (resp) {\n\n              // vm.item = resp.data\n              // console.log('aaaaaa',this.item.itemId);\n              vm.$message({\n                message: \"添加成功!\",\n                type: \"success\",\n              });\n              vm.$router.go(-1);\n            })\n            .catch(function (error) {\n              vm.$message.error(\"添加失败\");\n            });\n        }\n    },\n\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .box {\n  margin-left: 200px;\n  height: 400px;\n  width: 700px;\n  border: solid rgb(187, 186, 187);\n}\n</style>\n"]}]}