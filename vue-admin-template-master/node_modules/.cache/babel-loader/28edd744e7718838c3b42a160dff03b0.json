...row };\n       console.log(this.detail.createTime);\n         this.$message({\n            type: \"success\",\n            message: \"查看项目成功\",\n          });\n    },\n\n     detailitem(){\n        console.log('qqqqqqqqq');\n         this.dialogFormVisible = false;\n        },\n\n    // -------------------------------------------------\n  }\n}\n</script>\n"]}]}