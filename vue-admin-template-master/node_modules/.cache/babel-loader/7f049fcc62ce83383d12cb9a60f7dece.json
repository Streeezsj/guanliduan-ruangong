tax = datax;\n\n        //柱形图\n        const testOpton2 = {\n          title: {\n            text: \"统计不同分类数量\",\n          },\n          tooltip: {\n            trigger: \"axis\",\n            axisPointer: {\n              type: \"shadow\",\n            },\n          },\n          grid: {\n            left: \"3%\",\n            right: \"4%\",\n            bottom: \"3%\",\n            containLabel: true,\n          },\n          xAxis: [\n            {\n              type: \"category\",\n              data: data.scoreData,\n              axisTick: {\n                alignWithLabel: true,\n              },\n            },\n          ],\n          yAxis: [\n            {\n              type: \"value\",\n            },\n          ],\n          series: [\n            {\n              name: data.scoreData,\n              type: \"bar\",\n              barWidth: \"60%\",\n              data: data.numberData,\n            },\n          ],\n        };\n        const A = echarts.init(this.$refs.test2);\n        A.setOption(testOpton2);\n\n      }\n    }),\n      //接口2\n      FindType().then((res) => {\n        const { code, data } = res.data;\n        if (code === 200) {\n          this.typeList = data;\n          //条形图\n          const testOption3 = {\n            title: {\n              text: \"各个学院的项目数\",\n            },\n            tooltip: {\n              trigger: \"axis\",\n              axisPointer: {\n                type: \"shadow\",\n              },\n            },\n            legend: {},\n            grid: {\n              left: \"3%\",\n              right: \"4%\",\n              bottom: \"3%\",\n              containLabel: true,\n            },\n            xAxis: {\n              type: \"value\",\n              boundaryGap: [0, 0.01],\n            },\n            yAxis: {\n              type: \"category\",\n              //y轴数据\n              data: data.typeData\n            },\n            series: [\n              {\n                name: \"项目数量\",\n       